{
    "version": "0.2.0",
    "configurations": [        
        {
            "type": "node",
            "request": "launch",
            "name": "Debug app SalesOrder",
            "runtimeExecutable": "yarn",
            "runtimeArgs": ["dev"],
            "cwd": "${workspaceFolder}",
            "env": {
                "NODE_ENV": "development",
            },
            "skipFiles": ["<node_internals>/**"],
            "sourceMaps": true,
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
        }
    ]
}
